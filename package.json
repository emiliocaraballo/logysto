  "scripts": {
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "start": "node dist/index.js",
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc",
    "test": "npm run build && mocha dist/tests --exit",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:generate": "ts-node typeorm migration:generate -c seed -n "
  },
