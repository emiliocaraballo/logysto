    "jwt-decode": "^3.1.2",
    "mocha": "^9.1.4",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "serve-static": "^1.14.2",
    "supertest": "^6.2.2",
